/* pages/secne/secne.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #DCDCDC;
}
.container{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  text-align: center;
}
.list{
  width: 98%;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  margin-top: 30rpx;
  justify-content: space-between;
  align-items: center;
  text-align: center;
}

.list-item{
  width: 340rpx;
  height: 340rpx;
  margin-bottom: 20rpx;
  display: block;
  border: 10rpx solid white;
}

.item-image{
  height: 100%;
  width: 100%;
  
}